            javascript-bridge="__$_bridge", \
            content-script@start=${dir-path}/cleanup.js, blocks-image=yes, \
            blocking-url-patterns="intent://", \
            script-when-loaded=${web-prefix}__on_web_loaded, \
            style=web

=end sbml
